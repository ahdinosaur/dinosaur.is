{
  "name": "dinosaur.is",
  "version": "0.0.0",
  "description": "dinosaur.is website",
  "main": "index.js",
  "scripts": {
    "start": "budo src:bundle.js -d src --live -- -d",
    "bundle": "browserify src -g envify -g uglifyify",
    "build": "npm run --silent bundle -- -o src/bundle.js",
    "disc": "npm run --silent bundle -- --full-paths | discify --open",
    "push": "gh-pages -d src --repo git@github.com:ahdinosaur/ahdinosaur.github.io -b master -t -a",
    "deploy": "npm-run-all -s build push"
  },
  "browserify": {
    "transform": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/dinosaur.is.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/dinosaur.is/issues"
  },
  "homepage": "https://github.com/ahdinosaur/dinosaur.is#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.1.0",
    "disc": "^1.3.2",
    "envify": "^3.4.0",
    "gh-pages": "^0.11.0",
    "npm-run-all": "^1.6.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "jquery": "^2.2.3",
    "munge": "^0.1.7",
    "randomstring": "^1.1.5"
  }
}
